{
  "name": "next-homepage",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "clean": "rm -rf node_modules/.cache && rimraf out",
    "build": "(npm install || (npm audix fix && npm install)) && npx update-browserslist-db@latest && mkdir -p .next && next build",
    "dev": "npm run build && next dev",
    "start": "npm run build && next start",
    "export": "npm run build && next export",
    "deploy": "npm run clean && npm run export && touch out/.nojekyll && gh-pages -d out -t true"
  },
  "dependencies": {
    "next": "^14.2.5",
    "nextjs": "^0.0.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^5.2.0"
  },
  "devDependencies": {
    "babel-plugin-transform-define": "^2.1.4",
    "gh-pages": "^3.1.0",
    "html-react-parser": "^5.1.12",
    "rimraf": "^3.0.2"
  }
}
